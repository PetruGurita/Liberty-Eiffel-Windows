deferred class _EXTERNALS

inherit ANY undefine is_equal, copy end

feature {} -- External calls
feature -- size
	struct_size: INTEGER is
		external "C inline use <gtk/gtk.h>"
		alias "sizeof(Foo)"
		end
end
